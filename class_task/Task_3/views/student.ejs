<%- include('partials/header') %>
    <h2>Student Result</h2>
    <p>ID: <%= student.id %>
    </p>
    <p>Name: <%= student.name %>
    </p>
    <p>Marks: <%= student.marks %>
    </p>
    <p>Grade: <%= student.grade %>
    </p>
    <p>Status: <%= student.marks>= 40 ? "Pass" : "Fail" %></p>
    <a href="/students">Back to List</a>
    <%- include('partials/footer') %>